<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.Bank">
    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logo"
        android:supportsRtl="true"
        android:screenOrientation="portrait"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.Bank.ForgotPassword"></activity>
        <activity android:name="com.example.Bank.Settings" />
        <activity android:name="com.example.Bank.Cards" />
        <activity android:name="com.example.Bank.BenificiaryDetails" />
        <activity android:name="com.example.Bank.MoneyTransfer" />
        <activity android:name="com.example.Bank.HomePage" />
        <activity android:name="com.example.Bank.ChangeLanguage" />
        <activity android:name="com.example.Bank.MainActivityLogin">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.Bank.Passbook" />
    </application>
</manifest>